{
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "template": "node_modules/@escommunity/minami"
    },
    "source": {
        "include": [
            "package.json",
            "index.js",
            "src/addon.class.js",
            "src/callback.class.js",
            "src/stream.class.js",
            "src/utils.js"
        ],
        "exclude": [
            "node_modules",
            "client"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}
