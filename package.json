{
  "name": "@slimio/addon",
  "version": "0.15.0",
  "description": "Slim.IO Addon package",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublish": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc ava",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint src/**",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Addon.git"
  },
  "keywords": [
    "SlimIO",
    "monitoring",
    "addon"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Addon/issues"
  },
  "homepage": "https://github.com/SlimIO/Addon#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.3",
    "@types/es6-shim": "^0.31.39",
    "@types/node": "^10.12.10",
    "@types/zen-observable": "^0.8.0",
    "ava": "^0.25.0",
    "cross-env": "^5.2.0",
    "depcheck": "^0.6.11",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "jsdoc": "^3.5.5",
    "nyc": "^13.1.0",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/is": "^1.4.0",
    "@slimio/safe-emitter": "^1.0.3",
    "@slimio/scheduler": "^0.6.0",
    "@slimio/timer": "^1.0.0",
    "is-snake-case": "^0.1.0",
    "uuid": "^3.3.2",
    "zen-observable": "^0.8.11"
  }
}
