{
  "name": "@slimio/addon",
  "version": "0.22.0",
  "description": "Slim.IO Addon package",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint src/**",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Addon.git"
  },
  "keywords": [
    "SlimIO",
    "monitoring",
    "addon"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "src/*.js"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Addon/issues"
  },
  "homepage": "https://github.com/SlimIO/Addon#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@types/es6-shim": "^0.31.39",
    "@types/node": "^12.6.2",
    "@types/zen-observable": "^0.8.0",
    "ava": "^2.3.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.1",
    "husky": "^3.0.0",
    "jsdoc": "^3.6.2",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/error": "^0.2.0",
    "@slimio/is": "^1.5.1",
    "@slimio/logger": "^0.1.2",
    "@slimio/safe-emitter": "^1.0.6",
    "@slimio/scheduler": "^0.8.0",
    "@slimio/timer": "^1.0.2",
    "is-snake-case": "^0.1.0",
    "uuid": "^3.3.3",
    "zen-observable": "^0.8.14"
  }
}
